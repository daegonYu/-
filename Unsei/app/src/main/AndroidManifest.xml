<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.unsei">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>



    <!-- CAMERA 사용권한 획득 -->
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera.autofocus"/>

    <!-- 카메라 권한 사용 -->
    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/maincon2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/maincon2"
        android:supportsRtl="true"
        android:theme="@style/Theme.Unsei">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyAGp8g6HNfkc5_J-McYarxHKfjDpxBE7VA" />

        <activity android:name="com.journeyapps.barcodescanner.CaptureActivity"
            android:screenOrientation="fullSensor"
            tools:replace="android:screenOrientation"
            android:stateNotNeeded="true"/>


        <activity android:name=".ScanQR"/>
        <activity android:name=".money"/>
        <activity android:name=".lotto_number2"/>
        <activity android:name=".lotto_number"/>
        <activity android:name=".lotto_page"/>
        <activity android:name=".Rank"/>
        <activity android:name=".GoogleMaps"/>
        <activity android:name=".unsepage_0" />
        <activity android:name=".unsepage_1" />
        <activity android:name=".unsepage_2" />
        <activity android:name=".taro_0"/>
        <activity android:name=".taro_1"/>
        <activity android:name=".taro_2"/>
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>